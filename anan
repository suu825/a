PotionTool.GripUp = Vector3.new(1,0,0)
PotionTool.GripPos = Vector3.new(1.5, 0.5, -1.5)
PotionTool.Parent = plr.Backpack
PotionTool.Parent = plr.Character
else
ChangeToggleColor(PotionDi_Button)
end
else
