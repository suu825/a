Players = game:GetService("Players")
LocalPlayer = Players.LocalPlayer
function setAnchored(character, anchored)
for _, part in ipairs(character:GetDescendants()) do
if part:IsA("BasePart") then
part.Anchored = anchored
end
end
end
DefenseTab:AddToggle({
Name = "オート無敵",
Default = false,
Callback = function(state)
local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
isAnchored = state
setAnchored(character, isAnchored)
if isAnchored then
end
end
})
LocalPlayer.CharacterAdded:Connect(function(character)
character:WaitForChild("HumanoidRootPart")
setAnchored(character, isAnchored)
end)
D_p = 0.000005
DefenseTab:AddToggle({
Name = "アンチ掴む",
Color = Color3.fromRGB(255, 255, 255),
Default = false,
Save = false,
Flag = "antiGrab",
Callback = function(enabled)
if enabled then
autoStruggleCoroutine = RunService.Heartbeat:Connect(function()
local character = localPlayer.Character
if character and character:FindFirstChild("Head") then
local head = character.Head
local partOwner = head:FindFirstChild("PartOwner")
if partOwner then
Struggle:FireServer()
ReplicatedStorage.GameCorrectionEvents.StopAllVelocity:FireServer()
for _, part in pairs(character:GetChildren()) do
if part:IsA("BasePart") then
part.Anchored = true
end
end
while localPlayer.IsHeld.Value do
wait(D_p)
end
for _, part in pairs(character:GetChildren()) do
if part:IsA("BasePart") then
part.Anchored = false
end
end
end
end
end)
else
if autoStruggleCoroutine then
autoStruggleCoroutine:Disconnect()
autoStruggleCoroutine = nil
end
end
end
})
DefenseTab:AddSlider({
Name = "アンチ掴む、アンカータイム",
Min = -0.0000001,
Max = 15,
Color = Color3.fromRGB(255, 255, 255),
ValueName = ".",
Increment = 0.000001,
Default = D_p ,
Callback = function(value)
D_p  = value
end
})
local MenuToys = ReplicatedStorage:WaitForChild("MenuToys")
localPlayer.CharacterAdded:Connect(function(character)
playerCharacter = character
end)
local currentKunai = nil
local kunaiSpawningEnabled = false
local function spawnItemCf(itemName, cframe)
local rotation = Vector3.new(90, 0, 0)
local toy = ReplicatedStorage.MenuToys.SpawnToyRemoteFunction:InvokeServer(itemName, cframe, rotation)
return toy
end
local function attachKunaiToThigh()
if currentKunai and currentKunai.Parent then
return
end
local torso = playerCharacter:WaitForChild("Torso", 5)
local leftLeg = playerCharacter:FindFirstChild("Left Leg") or torso
if torso and leftLeg then
local kunaiPosition = torso.CFrame
* CFrame.new(-0.5, -torso.Size.Y / 2, 0)
* CFrame.Angles(math.rad(-100), 0, 0)
spawnItemCf("NinjaKunai", kunaiPosition)
for i = 1, 9999999999999999999 do
local toysFolder = workspace:FindFirstChild(localPlayer.Name .. "SpawnedInToys")
if toysFolder then
currentKunai = toysFolder:FindFirstChild("NinjaKunai")
if currentKunai then
break
end
end
task.wait()
end
if currentKunai then
currentKunai.PrimaryPart.Anchored = false
local weld = Instance.new("WeldConstraint")
weld.Part0 = currentKunai.PrimaryPart
weld.Part1 = leftLeg
weld.Parent = currentKunai.PrimaryPart
currentKunai.AncestryChanged:Connect(function(_, parent)
if not parent then
currentKunai = nil
attachKunaiToThigh()
end
end)
else
end
else
end
end
local function monitorKunai()
while kunaiSpawningEnabled do
if not (currentKunai and currentKunai.Parent) then
currentKunai = nil
attachKunaiToThigh()
end
task.wait()
end
end
DefenseTab:AddToggle({
Name = "アンチキック",
Default = false,
Callback = function(value)
kunaiSpawningEnabled = value
if value then
attachKunaiToThigh()
task.spawn(monitorKunai)
else
kunaiSpawningEnabled = false
end
end
})
Players = game:GetService("Players")
RunService = game:GetService("RunService")
Workspace = game:GetService("Workspace")
LocalPlayer = Players.LocalPlayer
enabled = false
range = 38888888888888888888888888888
connections = {}
function disablePart(part)
if part:IsA("BasePart") then
part.CanTouch = false
part.CanCollide = false
part.CanQuery = false
elseif part:IsA("TouchTransmitter") or part:IsA("WeldConstraint") then
part:Destroy()
end
end
function handleSpecialPart(part)
if part:IsA("Model") and (part.Name == "BallSnowball" or part.Name == "SoundPart") then
for _, descendant in ipairs(part:GetDescendants()) do
disablePart(descendant)
end
end
end
function handleSpecialPartsInRange(center)
for _, obj in ipairs(Workspace:GetDescendants()) do
if obj:IsA("Model") and (obj.Name == "BallSnowball" or obj.Name == "SoundPart") then
local primaryPart = obj.PrimaryPart or obj:FindFirstChildOfClass("BasePart")
if primaryPart and (primaryPart.Position - center).Magnitude <= range then
task.defer(handleSpecialPart, obj)
end
end
end
end
function setupFastHandler()
local lastProcessed = tick()
local connection = RunService.Heartbeat:Connect(function()
if enabled and LocalPlayer.Character then
local humanoidRootPart = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
if humanoidRootPart then
if tick() - lastProcessed > (1 / 500) then
handleSpecialPartsInRange(humanoidRootPart.Position)
lastProcessed = tick()
end
end
end
end)
table.insert(connections, connection)
end
function setupSpecialPartListener()
local connection = Workspace.DescendantAdded:Connect(function(descendant)
if enabled and descendant:IsA("Model") and (descendant.Name == "BallSnowball" or descendant.Name == "SoundPart") then
task.defer(handleSpecialPart, descendant)
end
end)
table.insert(connections, connection)
end
function disableCharacterCollision(character)
for _, part in ipairs(character:GetDescendants()) do
if part:IsA("BasePart") then
part.CanTouch = false
part.CanCollide = false
end
end
end
function setupCharacterListener()
LocalPlayer.CharacterAdded:Connect(function(character)
character:WaitForChild("HumanoidRootPart")
disableCharacterCollision(character)
end)
end
function handleExistingCharacter()
if LocalPlayer.Character then
disableCharacterCollision(LocalPlayer.Character)
end
end
function clearConnections()
for _, connection in ipairs(connections) do
if connection.Connected then
connection:Disconnect()
end
end
connections = {}
end
DefenseTab:AddToggle({
Name = "アンチ雪玉",
Default = false,
Callback = function(state)
enabled = state
if enabled then
handleExistingCharacter()
setupCharacterListener()
setupFastHandler()
setupSpecialPartListener()
else
clearConnections()
end
end
})
