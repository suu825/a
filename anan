if enabled then
ObjectAuraDCoroutine = coroutine.create(ObjectAuraD)
coroutine.resume(ObjectAuraDCoroutine)
elseif ObjectAuraDCoroutine then
coroutine.close(ObjectAuraDCoroutine)
ObjectAuraDCoroutine = nil
end
end
})
