isBlackHoleActive = false
local Folder
local Attachment1
arasuTab:AddToggle({
Name = "ブラックホール",
Default = false,
Callback = function(value)
isBlackHoleActive = value
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Workspace = game:GetService("Workspace")
local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local function initializeFolder()
if not Folder then
Folder = Instance.new("Folder", Workspace)
local Part = Instance.new("Part", Folder)
Part.Anchored = true
Part.CanCollide = false
Part.Transparency = 1
Attachment1 = Instance.new("Attachment", Part)
end
end
local function cleanupFolder()
if Folder then
Folder:Destroy()
Folder = nil
Attachment1 = nil
end
end
local function ForcePart(v)
if v:IsA("Part") and not v.Anchored and not v.Parent:FindFirstChild("Humanoid") and not v.Parent:FindFirstChild("Head") and v.Name ~= "Handle" then
for _, x in ipairs(v:GetChildren()) do
if x:IsA("BodyAngularVelocity") or x:IsA("BodyForce") or x:IsA("BodyGyro") or x:IsA("BodyPosition") or x:IsA("BodyThrust") or x:IsA("BodyVelocity") or x:IsA("RocketPropulsion") then
x:Destroy()
end
end
if v:FindFirstChild("Attachment") then v:FindFirstChild("Attachment"):Destroy() end
if v:FindFirstChild("AlignPosition") then v:FindFirstChild("AlignPosition"):Destroy() end
if v:FindFirstChild("Torque") then v:FindFirstChild("Torque"):Destroy() end
v.CanCollide = false
local Torque = Instance.new("Torque", v)
Torque.Torque = Vector3.new(0, 0, 0)
local AlignPosition = Instance.new("AlignPosition", v)
local Attachment2 = Instance.new("Attachment", v)
Torque.Attachment0 = Attachment2
AlignPosition.MaxForce = math.huge
AlignPosition.MaxVelocity = math.huge
AlignPosition.Responsiveness = 200
AlignPosition.Attachment0 = Attachment2
AlignPosition.Attachment1 = Attachment1
end
end
local function toggleBlackHole()
if isBlackHoleActive then
initializeFolder()
for _, v in next, Workspace:GetDescendants() do
ForcePart(v)
end
blackHoleConnection = Workspace.DescendantAdded:Connect(function(v)
if isBlackHoleActive then
ForcePart(v)
end
end)
spawn(function()
while isBlackHoleActive and RunService.RenderStepped:Wait() do
if Attachment1 and humanoidRootPart then
Attachment1.WorldCFrame = humanoidRootPart.CFrame
end
end
end)
else
if blackHoleConnection then
blackHoleConnection:Disconnect()
blackHoleConnection = nil
end
cleanupFolder()
end
end
toggleBlackHole()
end
})
arasuTab:AddToggle({
Name = "全員燃やす",
Default = false,
Color = Color3.fromRGB(255, 255, 255),
Save = false,
Callback = function(enabled)
if enabled then
fireAllCoroutine = coroutine.create(fireAll)
coroutine.resume(fireAllCoroutine)
else
if fireAllCoroutine then
coroutine.close(fireAllCoroutine)
fireAllCoroutine = nil
end
end
end
})
