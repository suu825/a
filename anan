if part_to_impulse then
part_to_impulse.Parent:BreakJoints()
end
end
end)
GrabTab:AddToggle({
Name = "上に上がる掴む",
Default = false,
Save = false,
Flag = "skyGrab",
Callback = function(Value)
enabled = Value
if enabled then
if not connection then
connection = game:GetService("Workspace").ChildAdded:Connect(function(model)
if model.Name == "GrabParts" then
local part_to_impulse = model:FindFirstChild("GrabPart")
if part_to_impulse and part_to_impulse:FindFirstChild("WeldConstraint") then
local bodyVelocity = Instance.new("BodyVelocity")
bodyVelocity.Parent = part_to_impulse
bodyVelocity.Velocity = Vector3.new(0, 20, 0)
bodyVelocity.MaxForce = Vector3.new(0, math.huge, 0)
end
end
end)
end
else
if connection then
connection:Disconnect()
connection = nil
end
end
end
})
